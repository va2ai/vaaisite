    &lt;!-- Logic --&gt;
    &lt;script&gt;
        // Modal Logic
        function openModal(type) {
            const modal = document.getElementById('modal');
            const title = document.getElementById('modal-title');

            if (type === 'consultation') {
                title.innerText = &quot;Book Your 15-Min Workflow Audit&quot;;
            } else if (type === 'assessment') {
                title.innerText = &quot;Request Risk + RAG Assessment&quot;;
            }

            modal.classList.remove('opacity-0', 'pointer-events-none');
            document.body.classList.add('modal-active');
        }

        function closeModal() {
            const modal = document.getElementById('modal');
            modal.classList.add('opacity-0', 'pointer-events-none');
            document.body.classList.remove('modal-active');
        }

        function submitForm() {
            const btn = document.querySelector('form button');
            const originalText = btn.innerText;
            btn.innerText = &quot;Sent!&quot;;
            btn.classList.add('bg-green-600', 'hover:bg-green-700');
            setTimeout(() =&gt; {
                closeModal();
                // Reset button style after close
                setTimeout(() =&gt; {
                    btn.innerText = originalText;
                    btn.classList.remove('bg-green-600', 'hover:bg-green-700');
                }, 500);
            }, 1000);
        }

        // Tab Switch Logic
        function switchTab(tabId) {
            // Buttons
            document.querySelectorAll('.tab-btn').forEach(btn =&gt; btn.classList.remove('active'));
            if(tabId === 'demo-live') document.getElementById('tab-btn-live').classList.add('active');
            if(tabId === 'demo-difference') document.getElementById('tab-btn-difference').classList.add('active');

            // Content
            document.querySelectorAll('.tab-content').forEach(content =&gt; {
                content.classList.remove('active');
                setTimeout(() =&gt; {
                    if(!content.classList.contains('active')) content.style.display = 'none';
                }, 300); // Match transition time
            });

            const activeContent = document.getElementById(tabId);
            activeContent.style.display = 'block';
            // Slight delay to allow display block to apply before opacity transition
            setTimeout(() =&gt; {
                activeContent.classList.add('active');
            }, 10);
        }

        // Demo Logic
        function runDemo() {
            const scanner = document.getElementById('scanner-line');
            const initial = document.getElementById('demo-initial');
            const results = document.getElementById('demo-results');
            const docViewer = document.getElementById('doc-viewer');

            // Start Animation
            initial.style.display = 'none';
            scanner.style.display = 'block';

            // Cycle through document tabs
            const tabs = [
                { name: 'BVA Decision', scroll: 0 },
                { name: 'RDS (Rating)', scroll: 800 },
                { name: 'CP Exam', scroll: 1600 }
            ];

            let currentTab = 0;

            // Switch tabs during scan
            const tabInterval = setInterval(() =&gt; {
                currentTab++;
                if (currentTab &lt; tabs.length) {
                    // Scroll to document section
                    docViewer.scrollTop = tabs[currentTab].scroll;

                    // Update tab styling
                    updateTabVisual(currentTab);
                } else {
                    clearInterval(tabInterval);
                }
            }, 650);

            // Wait for scan to complete
            setTimeout(() =&gt; {
                clearInterval(tabInterval);
                scanner.style.display = 'none';
                results.classList.remove('hidden');
                docViewer.scrollTop = 0; // Reset to top
                updateTabVisual(0); // Reset to first tab
            }, 2000);
        }

        function updateTabVisual(activeIndex) {
            // Reset all tabs to inactive state
            for (let i = 0; i &lt; 3; i++) {
                const tab = document.getElementById(`tab-${i}`);
                if (tab) {
                    if (i === activeIndex) {
                        tab.className = 'px-3 py-1.5 bg-white border border-slate-300 rounded-t text-xs font-semibold text-slate-900 flex items-center gap-1 cursor-pointer hover:bg-slate-100 transition';
                    } else {
                        tab.className = 'px-3 py-1.5 bg-slate-50 border border-slate-200 rounded-t text-xs text-slate-600 flex items-center gap-1 cursor-pointer hover:bg-slate-100 transition';
                    }
                }
            }
        }

        function switchDocTab(tabIndex) {
            const docViewer = document.getElementById('doc-viewer');
            const scrollPositions = [0, 1800, 3600]; // Approximate scroll positions for each document

            // Update tab visual
            updateTabVisual(tabIndex);

            // Scroll to document section
            if (docViewer) {
                docViewer.scrollTop = scrollPositions[tabIndex];
            }
        }

        function resetDemo() {
            const initial = document.getElementById('demo-initial');
            const results = document.getElementById('demo-results');
            const highlights = document.querySelectorAll('.highlight-text');

            initial.style.display = 'flex';
            results.classList.add('hidden');
            highlights.forEach(el =&gt; {
                el.outerHTML = el.innerHTML; // Remove span wrapper
            });
            // Reset scroll
            document.getElementById('doc-viewer').scrollTop = 0;
        }

        function highlight(spanId, paraId) {
            // Remove existing highlights
            document.querySelectorAll('.highlight-active').forEach(el =&gt; el.classList.remove('highlight-active'));

            // Scroll to paragraph
            const para = document.getElementById(paraId);
            if (para) {
                para.scrollIntoView({ behavior: 'smooth', block: 'center' });

                // Add highlight class
                const span = document.getElementById(spanId);
                // Simple highlight effect for demo (in production would use Ranges)
                if (span) {
                    span.classList.add('highlight-text', 'highlight-active');
                    setTimeout(() =&gt; {
                        span.classList.remove('highlight-active');
                    }, 2000);
                }
            }
        }

        // Sticky Nav Blur Effect
        window.addEventListener('scroll', () =&gt; {
            const nav = document.getElementById('navbar');
            if (window.scrollY &gt; 20) {
                nav.classList.add('shadow-md');
            } else {
                nav.classList.remove('shadow-md');
            }
        });
    &lt;/script&gt;

